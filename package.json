{
	"name": "mapshaper",
	"version": "0.6.14",
	"description": "A tool for editing vector datasets for mapping and GIS.",
	"keywords": [
		"shapefile",
		"topojson",
		"geojson",
		"cartography",
		"simplification",
		"topology",
		"csv",
		"gis"
	],
	"author": "Matthew Bloch <masiyou@gmail.com>",
	"contributors": [],
	"license": "MPL-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/mbloch/mapshaper"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"scripts": {
		"test": "mocha test",
		"build": "rollup --config",
		"lint": "eslint --ext mjs src/",
		"prepublishOnly": "npm lint; npm test; ./pre-publish",
		"postpublish": "./release_web_ui; ./release_github_version",
		"browserify": "browserify -r sync-request -r mproj -r buffer -r iconv-lite -r fs -r flatbush -r rw -r path -r kdbush -r @tmcw/togeojson -o www/modules.js",
		"dev": "rollup --config --watch"
	},
	"main": "./mapshaper.js",
	"files": [
		"/bin/**",
		"/www/**",
		"!/www/nacis/**",
		"/mapshaper.js",
		"!.DS_Store"
	],
	"dependencies": {
		"@tmcw/togeojson": "^4.7.0",
		"@xmldom/xmldom": "^0.8.2",
		"adm-zip": "^0.5.9",
		"commander": "7.0.0",
		"cookies": "^0.8.0",
		"d3-color": "3.1.0",
		"d3-scale-chromatic": "2.0.0",
		"delaunator": "^5.0.0",
		"flatbush": "^3.2.1",
		"geokdbush": "^1.1.0",
		"iconv-lite": "^0.6.3",
		"kdbush": "^3.0.0",
		"mproj": "0.0.35",
		"opn": "^5.3.0",
		"rw": "~1.3.3",
		"sync-request": "5.0.0",
		"tinyqueue": "^2.0.3"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.3.0",
		"browserify": "^17.0.0",
		"csv-spectrum": "^1.0.0",
		"eslint": "^8.16.0",
		"mocha": "^10.0.0",
		"rollup": "^2.73.0",
		"shell-quote": "^1.6.1",
		"underscore": "^1.13.1"
	},
	"mocha": {
		"reporter": "dot",
		"node-option": [
			"experimental-specifier-resolution=node"
		],
		"check-leaks": true,
		"parallel": true,
		"jobs": 4
	},
	"bin": {
		"mapshaper": "./bin/mapshaper",
		"mapshaper-gui": "./bin/mapshaper-gui",
		"mapshaper-xl": "./bin/mapshaper-xl"
	}
}
